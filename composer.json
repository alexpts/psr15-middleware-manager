{
    "name": "alexpts/psr15-middleware-manager",
    "description": "Middleware manager compatible with the PSR-7 and PSR-15",
    "homepage": "https://github.com/alexpts/psr15-middleware-manager",
    "license": "MIT",
    "type": "library",
    "optimize-autoloader": true,
    "keywords": [
        "middleware",
        "psr-7",
        "psr-15"
    ],
    "authors": [{
        "name": "Alexpts",
        "email": "alexpts3@gmail.com"
    }],
    "minimum-stability": "stable",
    "require": {
        "php": "~7.2",
        "psr/http-server-middleware": "^1.0",
        "psr/http-message": "^1.0",
        "alexpts/php-simple-events": "^3.0"
    },
    "autoload": {
        "psr-0": {
            "PTS\\PSR15\\MiddlewareManager": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "zendframework/zend-diactoros": "^1.7",
        "codeclimate/php-test-reporter": "^0.4.4"
    }
}
